{
  "appenders": {
    "console": {
        "type": "console"
    },
    "multi": {
      "type": "multiFile",
      "base": "./tmp",
      "property": "categoryName",
      "extension": ".log",
      "maxLogSize": 10485760
    }
  },
  "categories": {
    "default": {
      "appenders": ["multi", "console"],
      "level": "debug"
    }
  }
}